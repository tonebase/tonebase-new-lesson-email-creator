{{#data}}
<tbody>
  <tr>
    <td
      class="buttonCell"
      width="100%"
      align="center"
      valign="top"
      bgcolor="#E70000"
      style="border:solid 1px #e14f3d;background-color:#e14f3d;-moz-border-radius:4px;-webkit-border-radius:4px;border-radius:4px;display:inline-block;cursor:pointer;color:rgb(255, 255, 255);text-decoration:none;"
    >
      <table
        cellpadding="0"
        cellspacing="0"
        border="0"
        width="100%"
        align="center"
      >
        <tbody>
          <tr>
            <td
              class="viewAllButton"
              align="center"
              valign="middle"
              style="padding:11px 0px 11px 0px;font-family:Oswald,Helvetica, Arial, sans-serif;-webkit-text-size-adjust:none;font-weight:normal;font-size:16px;"
            >
              <a
                href="https://tonebase.co/artists/{{artist_slug}}"
                target="_blank"
                style="text-decoration:none;text-transform: uppercase;letter-spacing: 0.032em;color:rgb(255, 255, 255);display:block;"
              >
                VIEW ALL LESSONS &rarr;
              </a>
            </td>
          </tr>
        </tbody>
      </table>
    </td>
  </tr>
</tbody>
{{/data}}
