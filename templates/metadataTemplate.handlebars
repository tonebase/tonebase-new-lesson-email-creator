{{#data}}
<tbody>
  <tr>
    <td align="center">
      <table
        cellpadding="0"
        cellspacing="0"
        border="0"
        width="83%"
        align="center"
      >
        <tbody>
          <tr>
            <td
              class="metaData"
              align="center"
              style="color:#ffc82c;font-family:Oswald,Helvetica, Arial, sans-serif;text-transform:uppercase;letter-spacing:0.032em;-webkit-text-size-adjust:none;font-size:14px;line-height:28px;font-weight:normal;"
            >
            <span
              id="metadata_color_dyna"
            >
              <span id="type_insert_dyna"></span>
              <span style="color:#333;"
                >&nbsp;&nbsp;|&nbsp;&nbsp;</span
              >
              <span id="difficulty_insert_dyna"></span>
              <span style="color:#333;"
                >&nbsp;&nbsp;|&nbsp;&nbsp;</span
              >
              {{total_length}}
            </span>
            </td>
          </tr>
          <tr>
            <td
              class=""
              align="center"
              style="color:#FFFFFF;text-transform: uppercase; letter-spacing: 0.032em;font-family:Oswald,Helvetica, Arial, sans-serif;-webkit-text-size-adjust: none;font-size:22px;line-height:28px;font-weight:normal;padding:5px 0px 10px 0px;text-transform:uppercase;"
            >
              {{title}}
            </td>
          </tr>
          <tr>
            <td
              class="syn"
              align="center"
              style="color:#ccc;font-family:Helvetica, Arial, sans-serif;-webkit-text-size-adjust:none;font-size:16px;line-height:28px;font-weight:normal;padding:5px 0px 60px 0px;"
            >
              {{description}}...
              <a
                href="https://tonebase.co/lessons/player/{{slug}}"
                target="_blank"
                style="color:#ffffff;font-family:Oswald,Helvetica,Arial,sans-serif;text-transform:uppercase;letter-spacing:0.032em;color:#e14f3d;font-size:12px;"
                >See More</a
              >
            </td>
          </tr>
        </tbody>
      </table>
    </td>
  </tr>
</tbody>
{{/data}}
